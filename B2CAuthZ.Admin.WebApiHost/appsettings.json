{
  "AzureAdB2C": {
    "Instance": "https://b2c-tenant-name.b2clogin.com/",
    "ClientId": "admin-ui-app-id",
    "Domain": "b2c-tenant-name.onmicrosoft.com",
    "SignUpSignInPolicyId": "b2c_1_user_flow_for_login"
  },
  "AzureAdDirectoryAdmin": {
    "ClientId": "admin-service-app-id",
    "ClientSecret": "admin-service-app-secret",
    "TenantName": "b2c-tenant-name.onmicrosoft.com",
    "Scopes": "https://graph.microsoft.com/.default"
  },
  "OrganizationOptions": {
    "TenantIssuerName": "b2c-tenant-id.onmicrosoft.com",
    "OrgIdClaimName": "extension_OrgId",
    "OrgIdExtensionName": "extension_<b2c-extensions-app-id-no-dashes>_OrgId",
    "OrgRoleExtensionName": "extension_<b2c-extensions-app-id-no-dashes>_OrgRole",
    "UserFieldSelection": "id,userPrincipalName,displayName,givenName,surname,identities",
    "AppRoleAssignmentFieldSelection": "id,appRoleId,principalId,resourceId,resourceDisplayName,principalDisplayName",
    "ApplicationOrgAdministratorRoleId": "common-admin-role-id"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "putinstrumentationkeyhere"
  },
  "AllowedHosts": "*"
}